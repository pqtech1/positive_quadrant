<script>
    $(document).ready(function () {
        $("#movetop").hide();
        $(function toTop() {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 100) {
                    $('#movetop').fadeIn();
                } else {
                    $('#movetop').fadeOut();
                }
            });

            $('#movetop').click(function () {
                $('body,html').animate({
                    scrollTop: 0
                }, 800);
                return false;
            });
        });

    });
</script>
