  <script>
    document.addEventListener("DOMContentLoaded", function () {
      let toggleBtn = document.querySelector('.parentContainer .navbarBtn img');
      let navToOpen = document.querySelector('.parentContainer nav');

      if (toggleBtn && navToOpen) {
        // Check if elements are correctly selected
        console.log("Toggle button and nav elements found.");

        let isNavOpen = false;

        toggleBtn.addEventListener('click', function () {
          console.log("Button is clicked");

          if (isNavOpen) {
            navToOpen.style.display = 'none';
            isNavOpen = false;
          } else {
            navToOpen.style.display = 'block';
            isNavOpen = true;
          }
        });
      } else {
        console.error("Toggle button or nav element not found. Check your selectors.");
      }
    });




  </script>


  <script>
    $(function () {
      $(".dropdown").hover(
        function () {
          $('.dropdown-menu', this).stop(true, true).fadeIn("fast");
          $(this).toggleClass('open');
          // $('b', this).toggleClass("caret caret-up");                
        },
        function () {
          $('.dropdown-menu', this).stop(true, true).fadeOut("fast");
          $(this).toggleClass('open');
          // $('b', this).toggleClass("caret caret-up");                
        });
    });
  </script>

  <script>
    document.addEventListener("DOMContentLoaded", function () {
      setTimeout(function () {
        document.querySelector(".loader-wrapper").classList.add("hide");
      }, 500);
    });
  </script>



